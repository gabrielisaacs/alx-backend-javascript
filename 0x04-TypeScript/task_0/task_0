interface Student {
	firstName: string,
	LastName: string,
	age: number,
	location: string
}

const student1: Student = {
	firstName: 'Brian',
	LastName: 'Wakhile',
        age: 22,
        location: 'Kenya'
};

const student1: Student = {
        firstName: 'Mike',
        LastName: 'Onyeke',
        age: 23,
        location: 'Nigeria'
};

const studentsList: Student[] = [student1, student2];

const table = document.createElement('table');

const tableHeaderRow = table.insertRow();
const firstNameHeader = tableheaderRow.insertCell(0);
const studentLocationHeader = tableheaderRow.insertCell(1);
firstNameHeader.textContent = 'First Name';
studentLocationHeader.textContent = 'Location';

studentsList.forEach((student) => {
	const row = table.insertRow();
	const firstNameCell = row.insertCell(0);
	const studentLocationCell = row.insertCell(1);
	firstNameCell.textContent = student.firstName;
	studentLocationCell.textContent = student.location;
});

document.body.appendChild(table);

